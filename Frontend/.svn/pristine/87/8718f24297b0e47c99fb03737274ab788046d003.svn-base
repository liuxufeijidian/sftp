						<input type="hidden" name="partnerId"  id="partnerId"/>
						<input type="text" name="partnerName" id="partnerName" class="input-xs" required="">
					</label>
				</section>
				<section>
					<label class="label">合作商code</label>
					<label class="input">
						<input type="text" name="partnerCode" id="partnerCode" class="input-xs" required="">
					</label>
				</section>
				<section>
					<label class="label">地域code</label>
					<label class="input">
						<input type="text" name="areaCode" id="areaCode" class="input-xs" required="">
					</label>
				</section>
				<!--
				<section>
					<label class="label">合作商类型</label>
					<label class="input">
						<input type="text" name="partnerType" id="partnerType" class="input-xs" required="">
					</label>
				</section>-->
				<section>
					<label class="label">联系人</label>
					<label class="input">
						<input type="text" name="contact" id="contact" class="input-xs" required="">
					</label>
				</section>
				<section>
					<label class="label">联系电话</label>
					<label class="input">
						<input type="text" name="telephone" id="telephone" class="input-xs" required="">
					</label>
				</section>
				<section>
					<label class="label">邮件</label>
					<label class="input">
						<input type="text" name="email" id="email" class="input-xs" required="">
						<input type="hidden" name="state" id="state" value="1">
					</label>
				</section>
			</fieldset>
		</form>
	</fieldset>
</div>
