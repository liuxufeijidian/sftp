<div id="addTypeWindow">
	<fieldset>
		<form class="form-inline smart-form" id="addTypeForm" data-parsley-validate>
			<fieldset class="mySet" style="padding: 5px 14px 5px !important;background: none">
				<section>
					<label class="label">投诉内容</label>
					<label class="input">
						<input type="text" name="complainContent" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">回复内容</label>
					<label class="input">
						<input type="text" name="replyContent" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">回复人</label>
					<label class="input">
						<input type="text" name="replyName" class="input-xs form-control" required="">
					</label>
				</section>
			</fieldset>
		</form>
	</fieldset>
</div>
<!--编辑投诉窗口  -->
<div id="editTypeWindow">
	<fieldset>
		<form class="form-inline smart-form" id="editTypeForm" data-parsley-validate>
			<fieldset class="mySet" style="padding: 5px 14px 5px !important;background: none">
				<section>
					<label class="label">投诉内容</label>
					<label class="input">
						<input type="hidden" name="id" id="id" class="input-xs form-control" required="">
						<input type="text" name="complainContent" id="complainContent" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">回复内容</label>
					<label class="input">
						<input type="text" name="replyContent" id="replyContent" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">回复人</label>
					<label class="input">
						<input type="text" name="replyName" id="replyName" class="input-xs form-control" required="">
					</label>
				</section>
			</fieldset>
		</form>
	</fieldset>
</div>

<div id="addContWindow">
	<fieldset>
		<form class="form-inline smart-form" id="addConstForm" data-parsley-validate>
			<fieldset class="mySet" style="padding: 5px 14px 5px !important;background: none">
				<input type="hidden" id="constTypeId" name="constTypeId" class="input-xs form-control">
				<input type="hidden" name="upperValueId" class="input-xs form-control">
				<section>
					<label class="label">静态值</label>
					<label class="input">
						<input type="text" name="constValue" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">静态值描述</label>
					<label class="input">
						<input type="text" name="constDesc" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">回复人</label>
					<label class="input">
						<input type="text" name="sort" class="input-xs form-control" required="">
					</label>
				</section>
				<input type="hidden" name="dataValue" class="input-xs form-control">
			</fieldset>
		</form>
	</fieldset>
</div>
<div id="editContWindow">
	<fieldset>
		<form class="form-inline smart-form" id="editConstForm" data-parsley-validate>
			<fieldset class="mySet" style="padding: 5px 14px 5px !important;background: none">
				<input type="hidden" name="constTypeId" class="input-xs form-control">
				<input type="hidden" id="id" name="id" class="input-xs form-control">
				<section>
					<label class="label">静态值</label>
					<label class="input">
						<input type="text" name="constValue" id="constValue" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">静态值描述</label>
					<label class="input">
						<input type="text" name="constDesc" id="constDesc" class="input-xs form-control" required="">
					</label>
				</section>
				<section>
					<label class="label">排序</label>
					<label class="input">
						<input type="text" name="sort" id="sort" class="input-xs form-control" required="">
					</label>
				</section>
			</fieldset>
		</form>
	</fieldset>
</div>
