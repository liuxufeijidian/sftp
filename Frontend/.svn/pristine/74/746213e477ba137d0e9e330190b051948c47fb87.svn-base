@charset "utf-8";
.community-select>div:nth-of-type(1),
.community-select>div:nth-of-type(2){
	margin-bottom:15px;
}
.community-select input[type="text"],
.community-select>div:nth-of-type(3) div{
	background-image:url(/modules/base/images/split-line.png?__spirite);
	background-position:right center;
	background-size:1px 85%;
	padding:0px 7px;
	}
.community-select input[type="submit"],
.community-select>div:nth-of-type(3) a{
	height:100%;
	background-image:url(/modules/base/images/search.png?__spirite);
	width:30px;
	background-size:60% auto;
	background-position:center;
	background-repeat:no-repeat;
	}
.community-select ul li{
	border-bottom:1px solid rgba(255,255,255,0.3);
	line-height:220%;
	}
.community-select>div:nth-of-type(3) a{
	background-image:url(/modules/base/images/locate.png?__spirite);
	height:2rem;
	background-size:auto 60%;
	}
.community-select>div:nth-of-type(3) div{
	height:2rem;
	padding:0px 15px 0px 15px;
	}
article{
	padding-top:30px;
	margin-top:15px;
	}
article>div{
	margin-top:20px;
	}
h1{
	text-align:center;
	font-weight:normal;
	}
article h1{
	margin-bottom:10px;
}

.swiper-container .swiper-pagination-bullet{
	background:none;
	border:2px solid rgba(255,255,255,0.3);
	opacity:1;
	}
.swiper-container .swiper-pagination-bullet-active{
	background:white;
	}
.swiper-container{
	width:100%;
	overflow:hidden;
	border-radius:10px;
	background-color:black;
	margin-top:15px;
	}
.swiper-container .swiper-slide{
	padding:0px;
	display:-webkit-flex;
	-webkit-align-items:center;
}
.swiper-container img{
	width:100%;
	margin:0px;
	top:0px;
	}
.wash-bg:before{
	content:"";
	background-image:url(/modules/base/images/car-wash.png?__spirite);
	display:block;
	width:2.5rem;
	height:2rem;
	background-repeat:no-repeat;
	background-size:auto 100%;
	}
.items-width-euqal{
	margin-top:5px;
	line-height:normal;
	}
.items-width-euqal div{
	width:30%;
	text-align:center;
	padding:8px 10px;
	}
.items-width-euqal>div:first-child{
	text-align:left;
	}
.order-fill {
	padding-top:40px;
}
.order-fill .fill-title{
	padding:0px 7px;
	border-right:1px solid rgba(255,255,255,0.6);
	white-space:nowrap;
	}
.order-fill .fill-content{
	margin:0px 7px;
	width:30px;
	}
.order-fill .equal-width:first-child{
	margin-right:20px;
	}
.order-fill .equal-width:only-child{
	margin-right:0px;
	}
.order-fill>div,
.car-list>div>div{
	margin-bottom:20px;
	}
.order-fill textarea{
	height:50px;
	font-size:0.8rem;
	color:white;
	
	}
.order-fill .remark>div{
    padding:7px;
	}
.order-fill .remark .fill-title{
	border:none;
	}
.car-type{
	text-align:center;
	position:relative;
	height:100%;
	margin:0px;
	}
.order-fill .car-type:first-child{
	margin:0px;
	}
.car-type input[type="radio"]{
	width:100%;
	height:100%;
	position:absolute;
	left:0px;
	top:0px;
	box-sizing: border-box;
	
	border-radius:0px;
	}
.car-type input[type="radio"]:checked{
	background-color:rgba(255,255,255,0.3);
	
	}
.add-car{
	position:relative;
	margin-bottom:30px;
	}
.add-car .circle{
	position:absolute;
	left:0px;
	top:0px;
	border:1px solid rgba(255,255,255,0.2);
	}
.add-car .flex-grow{
	margin-left:1rem;
	border-left:none;
	border-radius:0 0.3rem 0.3rem 0;
	}
.order-fill h2{
	margin-bottom:30px;
	}
.order-info>div{
	margin-bottom:7px;
	}
.fixed-bottom>:only-child{
	border-right:none;
	}
.add:before{
	content:"";
	display:block;
	width:1rem;
	height:1rem;
	border:1px solid white;
	border-radius:100%;
	background-image:url(/modules/base/images/add.png?__spirite);
	background-size:50% auto;
	background-repeat:no-repeat;
	background-position:center;
	margin-right:0.3rem;
	}
.evaluation{
	margin-top:4rem;
	}
.photo-wrapper{
	height:10rem;
	position:relative;
	}
.photo-wrapper .add:before{
	width:1.7rem;
	height:1.7rem;
	background-color:rgba(255,255,255,0.3);
	border:none;
	box-shadow:2px 2px 10px rgba(0,0,0,0.3);
	} 
.photo-wrapper input{
	position:absolute;
	width:100%;
	height:100%;
	left:0px;
	top:0px;
	opacity:0;
	}
.photo-wrapper[class~="white-bg"]{
	 background-color:white;
	}
.photo-wrapper .close{
	position:absolute;
	width:1.5rem;
	height:1.5rem;
	top:-0.75rem;
	right:-0.75rem;
	border-radius:100%;
	background-color:rgba(0,0,0,0.5);
	font-size:1.5rem;
	}
.photo-wrapper img{
	max-height:90%;
	max-width:90%;
	}
.intro-spec{
	margin-top:1rem;
	}
.intro-spec>div{
	margin:0px 1rem 2rem 1rem;
	}
.intro-spec h2{
	padding:0.3rem 0.8rem;
	margin-bottom:0.7rem;
	}
.counts-num{
	width:3rem;
	text-align:center;
	}
.poster{
	margin:0 20px;
	}
.poster img{
	width:100%;
	}
.intro-spec .pet-manager-intro{
	margin:0 auto;
	width:95vw;
	position:relative;
	left:-10px;
}

.flow-layout{
	margin-top:1rem;
	}
.flow-layout>div{
	width:100%;
	}
.flow-layout a{
	width:25vw;
	height:25vw;
	margin:4%;
	min-width:100px;
	min-height:100px;
	}
.container .car-manager-flow a{
	 width:22vw;
	 height:22vw;
	 min-width:85px;
	 min-height:90px;
	 margin:2vw;
	 }
.flow-layout a div:before{
	content:"";
	display:block;
	width:4rem;
	height:4rem;
	background-repeat:no-repeat;
	background-size:100% auto;
	margin:0 auto;
	background-position:center center;
	}
.flow-layout a div{
	text-align:center;
	}
.pet-beauty-bg div:before{
	background-image:url(/modules/base/images/drier.png?__spirite);
	}
.pet-moll div:before{
	background-image:url(/modules/base/images/bag.png?__spirite);
	}
.pet-activity div:before{
	background-image:url(/modules/base/images/present.png?__spirite);
	}
.pet-problem div:before{
	background-image:url(/modules/base/images/help.png?__spirite);
	}
.car-beauty div:before{
	background-image:url(/modules/base/images/car-beauty.png?__spirite);
	}
.client-service div:before{
	background-image:url(/modules/base/images/phone.png?__spirite);
	}
.personal-center div:before{
	background-image:url(/modules/base/images/person.png?__spirite);
	}
.intro-list h2{
	padding:0px;
	}
.intro-list>div>div{
	margin-left:1.2rem;
	}
.intro-list>div>div>div{
	margin-bottom:15px;
	}
.intro-list h2 .icon{
	margin-right:0.2rem;
	}
.car-manager-menu{
	overflow:hidden;
	width:74.5vw;
	margin: 30px auto 10px auto;
	}
.car-manager-menu>div:first-child{
	width:6rem;
	background-image:url(/modules/base/images/car-wash.png?__spirite);
	background-position:center;
	background-size:70% auto;
	background-position:center;
	}
.car-manager-menu ul{
	position:relative;
	margin:0px;
	}
.car-manager-menu li{
	margin-bottom:5px;
	padding:5px 0;
	position:relative;
	
	}
.car-manager-menu li:last-child{
	margin-bottom:0px;
	}
.car-manager-index .bg{
	background-color:rgba(255,255,255,0.2)
}	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	